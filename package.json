{
  "name": "hybrid-api-ts",
  "version": "1.4.4",
  "description": "API for Hybrid. Insite application built with TypeScript, Express, and TypeORM.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"ts-node ./src/index.ts\" ",
    "build": "tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "node ./node_modules/typeorm/cli.js -d ./dist/app-data-source.js",
    "migration:create": "npx typeorm migration:create ./src/migration/migration",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/app-data-source.ts ./src/migration/migration",
    "migration:run": "npm run typeorm -- migration:run -d ./src/app-data-source.ts",
    "migration:run:prod": "npm run typeorm:prod -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/app-data-source.ts",
    "migration:revert:prod": "npm run typeorm:prod -- migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/hybrid-insite/express-api-hybrid.git"
  },
  "keywords": [
    "hybrid",
    "api",
    "insiteve"
  ],
  "author": "Dey Gordillo",
  "license": "See LICENSE",
  "bugs": {
    "url": "https://gitlab.com/hybrid-insite/express-api-hybrid/issues"
  },
  "homepage": "https://gitlab.com/hybrid-insite/express-api-hybrid#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/nodemailer": "^7.0.4",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.2",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.2",
    "nodemailer": "^7.0.12",
    "reflect-metadata": "^0.1.13",
    "resend": "^6.6.0",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21"
  }
}
