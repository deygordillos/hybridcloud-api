{
  "name": "hybrid-api-ts",
  "version": "1.0.0",
  "description": "API for Hybrid Systems - Insite Venezuela",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\" ",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm migration:create -- --dataSource ./src/app-data-source.ts",
    "migration:generate": "npm run typeorm migration:generate -- --dataSource ./src/app-data-source.ts --outputJs ./src/migration/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/app-data-source.ts migration:run",
    "migration:revert": "typeorm-ts-node-commonjs -d ./src/app-data-source.ts migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/hybrid-insite/express-api-hybrid.git"
  },
  "keywords": [
    "hybrid",
    "api",
    "insiteve"
  ],
  "author": "Insite Venezuela",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/hybrid-insite/express-api-hybrid/issues"
  },
  "homepage": "https://gitlab.com/hybrid-insite/express-api-hybrid#readme",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}
